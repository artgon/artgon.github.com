---
layout: nil
---

<?xml version="1.0"?>
<feed xmlns="http://www.w3.org/2005/Atom">
 
  <title>Arthur Gonigberg</title>
  <link href="http://arthur.gonigberg.com/"/>
  <link type="application/atom+xml" rel="self" href="http://arthur.gonigberg.com/atom.xml"/>
  <updated>{{ site.time | date_to_xmlschema }}</updated>
  <id>http://arthur.gonigberg.com</id>
  <author>
    <name>Arthur Gonigberg</name>
    <email>arthur@gonigberg.com</email>
  </author>

  {% for post in site.posts limit: 10 %}
  <entry>
    <id>{{site.url}}{{ post.id }}</id>
    <link type="text/html" rel="alternate" href="{{ site.url }}{{ post.url }}"/>
    <title>{{ post.title }}</title>
    <updated>{{ post.date | date_to_xmlschema }}</updated>
    <author>
      <name>Arthur Gonigberg</name>
      <uri>http://arthur.gonigberg.com/</uri>
    </author>
    <content type="html">{{ post.content | xml_escape }}</content>
  </entry>
  {% endfor %}
 
</feed>
